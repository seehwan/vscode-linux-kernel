{
    "files.exclude": {
        "**/.*.*.cmd": true,
        "**/.*.d": true,
        "**/.*.o": true
    },
    "[c]": {
        "editor.detectIndentation": false,
        "editor.tabSize": 8,
        "editor.insertSpaces": false,
        "editor.rulers": [80,100]
    },
    "files.associations": {
        "*.h": "c"
    },
    "C_Cpp.default.compilerPath": "/usr/local/bin/aarch64-linux-gnu-gcc",
    "C_Cpp.default.intelliSenseMode": "linux-gcc-arm64",
}
